{
  "root": true,
  "extends": ["react-app", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": ["warn"]
  },
  "overrides": [
    {
      "files": ["**/*.ts?(x)"],
      "rules": {
        "prettier/prettier": [
          "warn",
          {
            "semi": true
          }
        ]
      }
    }
  ]
}
